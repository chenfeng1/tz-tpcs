<?xml version="1.0"?>
<mapping>
	<description>Description of the mapping</description>
		
	<class name="com.tz.tpcs.xml.Mobile">
	   <map-to xml="mobile-device"/>
	   <field name="name" type="string">
	      <bind-xml name="name" node="attribute"/>
	   </field>
	   <field name="model" type="string">
	      <bind-xml name="model" node="element"/>
	   </field>
	   <field name="price" type="double">
	      <bind-xml name="price" node="element"/>
	   </field>
	</class>
	
</mapping>